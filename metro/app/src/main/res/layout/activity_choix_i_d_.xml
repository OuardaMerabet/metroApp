<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/drawer_layout"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context=".authentis.UI.choixID_Activity">

  <LinearLayout
      android:id="@+id/formatDoc"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:orientation="vertical"
      android:visibility="visible">

      <include layout="@layout/toolbar_main_authentis" />
      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_above="@id/formatId"
          android:fontFamily="@font/avenir_black"
          android:text="@string/title_select_document"
          android:textColor="@color/black"
          android:textSize="31sp"
          android:textStyle="bold"
          android:layout_marginLeft="20dp"/>

      <RelativeLayout
          android:id="@+id/formatId"
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent"
          android:layout_marginTop="10dp"
          android:layout_marginBottom="10dp"
          tools:context=".UI.choixID_Activity">

          <TextView
              android:id="@+id/text_select_document"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginLeft="20dp"
              android:layout_marginRight="20dp"
              android:justificationMode="inter_word"
              android:fontFamily="@font/avenir_book"
              android:text="@string/text_select_document"
              android:textColor="#727d8c"
              android:textSize="15sp" />

          <LinearLayout
              android:id="@+id/cartesID"
              android:layout_below="@+id/text_select_document"
              android:layout_width="match_parent"
              android:layout_height="130dp"
              android:orientation="horizontal"
              android:gravity="center_vertical"
              android:layout_margin="20dp">
              <CheckBox
                  android:id="@+id/check_card"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_weight="1"
                  android:background="@drawable/checkbox_card"
                  android:button="@color/white"
                  android:clickable="true"
                  android:focusable="true"
                  android:layout_margin="15dp"/>
              <CheckBox
                  android:id="@+id/check_passeport"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:background="@drawable/checkbox_passport"
                  android:button="@color/white"
                  android:layout_weight="1"
                  android:clickable="true"
                  android:layout_margin="15dp"
                  android:focusable="true" />
          </LinearLayout>

          <LinearLayout
              android:id="@+id/permis"
              android:layout_width="match_parent"
              android:layout_height="130dp"
              android:layout_below="@+id/cartesID"
              android:layout_marginStart="20dp"
              android:layout_marginLeft="20dp"
              android:layout_marginTop="0dp"
              android:layout_marginEnd="20dp"
              android:layout_marginRight="20dp"
              android:layout_marginBottom="20dp"
              android:gravity="center_vertical"
              android:orientation="horizontal">

              <CheckBox
                  android:id="@+id/check_permis"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_margin="15dp"
                  android:background="@drawable/checkbox_permis"
                  android:button="@color/white"
                  android:layout_weight="1"
                  android:clickable="true"
                  android:focusable="true" />
              <ImageView
                  android:id="@+id/check_permis1"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_margin="15dp"
                  android:layout_weight="1"
                  android:background="@drawable/checkbox_permis"
                  android:button="@color/white"
                  android:visibility="invisible" />
          </LinearLayout>

          <ImageButton
              android:id="@+id/selectdocument_button"
              android:layout_above="@+id/buttom_bar"
              android:layout_width="wrap_content"
              android:layout_height="100dp"
              android:layout_centerHorizontal="true"
              android:background="@drawable/selectdocument_button"
              android:scaleType="fitXY" />

          <ImageView
              android:id="@+id/buttom_bar"
              android:layout_width="250dp"
              android:layout_height="15dp"
              android:layout_alignParentBottom="true"
              android:layout_centerHorizontal="true"
              android:src="@drawable/buttom_bar" />
      </RelativeLayout>

  </LinearLayout>

    <RelativeLayout
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:layout_gravity="right"
        android:background="@android:color/white">
        <include
            layout="@layout/activity_settings"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/startNFC"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#242424"
        android:orientation="vertical"
        android:visibility="gone">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="50dp">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="300dp"
                android:layout_height="240dp"
                app:srcCompat="@drawable/authentis" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/bottom_part"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            android:layout_alignParentBottom="true"
            android:background="@drawable/ntf">
            <TextView
                android:id="@+id/textView"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:text="Prêt à scanner"
                android:textColor="#21F3B1"
                android:textSize="24dp" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="121dp"
                android:layout_below="@+id/textView"
                android:layout_marginTop="20dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/nfc" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/imageView2"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="30dp"
                android:text=" Faites passer le Document sur
                             le lecteur NFC de votre téléphone"
                android:gravity="center"
                android:textColor="#79727572"
                android:textSize="19dp" />

            <ImageView
                android:id="@+id/buttom_bar2"
                android:layout_width="250dp"
                android:layout_height="15dp"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:src="@drawable/buttom_bar" />

        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/nfcScan"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#101010"
        android:backgroundTint="#101010"
        android:visibility="gone">

        <RelativeLayout
            android:id="@+id/top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="50dp">

            <ImageView
                android:layout_width="300dp"
                android:layout_height="240dp"
                app:srcCompat="@drawable/authentis" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/loading_layout"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            android:layout_alignParentBottom="true"
            android:background="@drawable/ntf">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:text="Scan en cours "
                android:textColor="#21F3B1"
                android:textSize="24dp" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="121dp"
                android:layout_below="@+id/textView1"
                android:layout_marginTop="20dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/nfc" />

            <ProgressBar
                android:id="@+id/progressbar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/imageView3"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="15dp"
                android:foregroundTint="#21F3B1"
                />

            <ImageButton
                android:id="@+id/button_annuler"
                android:layout_width="wrap_content"
                android:layout_height="100dp"
                android:layout_above="@+id/buttom_bar3"
                android:layout_centerHorizontal="true"
                android:background="@drawable/nfc_scan"
                android:scaleType="fitXY"
                android:layout_centerInParent="true" />
            <ImageView
                android:id="@+id/buttom_bar3"
                android:layout_width="250dp"
                android:layout_height="15dp"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:src="@drawable/buttom_bar" />
        </RelativeLayout>

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/results"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical"
       android:visibility="gone">

        <include layout="@layout/toolbar_result_details" />


        <androidx.cardview.widget.CardView
            android:layout_width="180dp"
            android:layout_height="230dp"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/margin_TopBottom"
            android:elevation="20dp"
            app:cardCornerRadius="250dp"
            android:innerRadius="0dp"
            android:shape="ring"
            android:thicknessRatio="1.9"
            app:cardBackgroundColor="#fff">
            <ImageView
                android:id="@+id/view_photo"
                android:layout_height="250dp"
                android:layout_width="250dp"
                android:layout_gravity="center_horizontal"
                android:src="@drawable/imgg"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true">
            </ImageView>
        </androidx.cardview.widget.CardView>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:background="#345A5959"
                    android:fontFamily="@font/avenir_medium"
                    android:paddingStart="20dp"
                    android:paddingTop="1dp"
                    android:paddingBottom="1dp"
                    android:text="@string/result_text1"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    tools:ignore="RtlSymmetry" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="5dp"
                        android:textColor="@color/black"
                        android:fontFamily="@font/avenir_medium"
                        android:text="@string/result_text2"
                        android:layout_weight="1"
                        android:textSize="16sp"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:layout_marginLeft="5dp"
                        android:textColor="@color/green"
                        android:fontFamily="@font/avenir_medium"
                        android:layout_weight="1"
                        android:text="         "
                        android:textSize="18sp"/>
                </LinearLayout>

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_TopBottom"
                    android:background="#345A5959"
                    android:fontFamily="@font/avenir_medium"
                    android:paddingStart="20dp"
                    android:paddingTop="@dimen/small_padding"
                    android:paddingBottom="@dimen/small_padding"
                    android:text="@string/result_text3"
                    android:textColor="@color/black"
                    android:textSize="@dimen/backButton_text_size"
                    tools:ignore="RtlSymmetry" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_LeftRight"
                            android:layout_marginRight="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:text="@string/result_text4"
                            android:layout_weight="1"
                            android:textSize="@dimen/normal_text_size"/>
                        <TextView
                            android:id="@+id/nameFirstname"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_LeftRight"
                            android:layout_marginLeft="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:layout_weight="1"
                            android:text="Said EL HACHIMI"
                            android:textSize="@dimen/normal_text_size"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_LeftRight"
                            android:layout_marginRight="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:text="@string/result_text5"
                            android:layout_weight="1"
                            android:textSize="@dimen/normal_text_size"/>
                        <TextView
                            android:id="@+id/output_last_name"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_LeftRight"
                            android:layout_marginLeft="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:layout_weight="1"
                            android:text="Said"
                            android:textSize="@dimen/normal_text_size"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_LeftRight"
                            android:layout_marginRight="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:text="@string/result_text6"
                            android:layout_weight="1"
                            android:textSize="@dimen/normal_text_size"/>
                        <TextView
                            android:id="@+id/output_first_name"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_LeftRight"
                            android:layout_marginLeft="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:layout_weight="1"
                            android:text="EL HACHIMI"
                            android:textSize="@dimen/normal_text_size"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_LeftRight"
                            android:layout_marginRight="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:text="@string/result_text7"
                            android:layout_weight="1"
                            android:textSize="@dimen/normal_text_size"/>
                        <TextView
                            android:id="@+id/output_gender"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_LeftRight"
                            android:layout_marginLeft="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:layout_weight="1"
                            android:text="Homme"
                            android:textSize="@dimen/normal_text_size"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:id="@+id/output_groupeSinguinField">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_LeftRight"
                            android:layout_marginRight="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:text="@string/result_text16"
                            android:layout_weight="1"
                            android:textSize="@dimen/normal_text_size"/>
                        <TextView
                            android:id="@+id/output_groupeSinguin"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_LeftRight"
                            android:layout_marginLeft="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:layout_weight="1"
                            android:text="B+"
                            android:textSize="@dimen/normal_text_size"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_LeftRight"
                            android:layout_marginRight="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:text="@string/result_text8"
                            android:layout_weight="1"
                            android:textSize="@dimen/normal_text_size"/>
                        <TextView
                            android:id="@+id/output_nationality"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_LeftRight"
                            android:layout_marginLeft="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:layout_weight="1"
                            android:text="Algérienne"
                            android:textSize="@dimen/normal_text_size"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_LeftRight"
                            android:layout_marginRight="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:text="@string/result_text9"
                            android:layout_weight="1"
                            android:textSize="@dimen/normal_text_size"/>
                        <TextView
                            android:id="@+id/output_birthDate"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_LeftRight"
                            android:layout_marginLeft="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:layout_weight="1"
                            android:text="22. 06. 1984"
                            android:textSize="@dimen/normal_text_size"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_LeftRight"
                            android:layout_marginRight="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:text="@string/result_text13"
                            android:layout_weight="1"
                            android:textSize="@dimen/normal_text_size"/>
                        <TextView
                            android:id="@+id/output_birthPlace"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_LeftRight"
                            android:layout_marginLeft="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:layout_weight="1"
                            android:text="Alger,Algerie"
                            android:textSize="@dimen/normal_text_size"/>
                    </LinearLayout>

                </LinearLayout>

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_TopBottom"
                    android:background="#345A5959"
                    android:fontFamily="@font/avenir_medium"
                    android:paddingStart="20dp"
                    android:paddingTop="@dimen/small_padding"
                    android:paddingBottom="@dimen/small_padding"
                    android:text="@string/result_text10"
                    android:textColor="@color/black"
                    android:textSize="@dimen/backButton_text_size"
                    tools:ignore="RtlSymmetry" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_LeftRight"
                            android:layout_marginRight="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:text="@string/result_text11"
                            android:layout_weight="1"
                            android:textSize="@dimen/normal_text_size"/>
                        <TextView
                            android:id="@+id/IDdoc"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_LeftRight"
                            android:layout_marginLeft="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:layout_weight="1"
                            android:text="0154234567"
                            android:textSize="@dimen/normal_text_size"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_LeftRight"
                            android:layout_marginRight="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:text="@string/result_text12"
                            android:layout_weight="1"
                            android:textSize="@dimen/normal_text_size"/>
                        <TextView
                            android:id="@+id/output_state"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_LeftRight"
                            android:layout_marginLeft="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:layout_weight="1"
                            android:text="Algérie"
                            android:textSize="@dimen/normal_text_size"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_LeftRight"
                            android:layout_marginRight="@dimen/margin_between_text"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_medium"
                            android:text="@string/result_text14"
                            android:layout_weight="1"
                            android:textSize="@dimen/normal_text_size"/>
                        <TextView
                            android:id="@+id/output_expirationDate"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_LeftRight"
